    "def solution(citations):\n",
    "    \n",
    "    answer = 0\n",
    "    \n",
    "    # 역정렬\n",
    "    revCit = sorted(citations, reverse=True)\n",
    "    \n",
    "    # 1. 순차비교\n",
    "    for i in range(len(revCit)):\n",
    "        if (i+1) <= revCit[i]:\n",
    "            answer += 1\n",
    "    \n",
    "    return answer"
   ]
  }
