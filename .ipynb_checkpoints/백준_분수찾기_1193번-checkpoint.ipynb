   "source": [
    "n = int(input())\n",
    "\n",
    "count = 1\n",
    "pre_sum = 1\n",
    "\n",
    "while pre_sum < n:\n",
    "    count += 1\n",
    "    pre_sum += count\n",
    "\n",
    "pre_sum -= count\n",
    "\n",
    "if count % 2 == 0: # 짝수\n",
    "    top = n - pre_sum\n",
    "    bottom = count - top + 1\n",
    "else:\n",
    "    bottom = n - pre_sum\n",
    "    top = count - bottom + 1\n",
    "\n",
    "print(f'{top}/{bottom}')"
   ]
  }
 ],
